# ~/.ssh/config

Host github.com
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

Host 470
  HostName t470s.local
  User dimitri
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  LocalForward 5001 localhost:5001
  LocalForward 8114 localhost:8114
  LocalForward 8116 localhost:8116
  LocalForward 8500 localhost:8500
  LocalForward 8502 localhost:8502
